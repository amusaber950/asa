    <UserLang name="ARC86" ext="arc">
        <Settings>
            <Global caseIgnored="no" />
            <TreatAsSymbol comment="no" commentLine="yes" />
            <Prefix words1="no" words2="no" words3="no" words4="no" />
        </Settings>
        <KeywordLists>
            <Keywords name="Delimiters">000,00</Keywords>
            <Keywords name="Folder+"></Keywords>
            <Keywords name="Folder-"></Keywords>
            <Keywords name="Operators">% ( ) , : +</Keywords>
            <Keywords name="Comment">1 2 0;</Keywords>
            <Keywords name="Words1">L GON GOP GO EXEC EXECN EXECP RTN REPT XCAL0 XCALL CCALL GOON EXECON</Keywords>
            <Keywords name="Words2">EWV EBV EQU IDLE SW SB CNTB CNTW CNTSB CNTSW CNTIW MUX FILLZ INIZB INIZW VSVD VDVS WTOBB BBTOW SENDDM GETDM SENDML AND ANDC ANDM OR ORC ORM EXOR NAND NOR NOT RS ENCOD DECOD COMPA BAR ANDB ORB XORB SUM SUMD DIFF DIFFD MULT MULTD MULA DIVD DIVA PROP SQRT MOY DELAY COUNT CLOCK SETTIM PC SLIM GATE ABS INTG INF SUP SWITCH SELECT TESTZ SHIFT PERTUI LATCH SMOY SWITD GATED SUSP CYCL FCYCL WAIT WSIG SIMP SIMG TSIG ISC ASCII BINASC HASBIN BINHAS ASCBIN READ WRITE CKSUM BINHEX HEXBIN SETUP FIND RARRAY WARRAY MADD MSUM ARSUM MNEG MULTM ARMUL MDIV ARDIV PROPM PROPD MSUP MINF OVER ARSWIT MMOY ARSDP RDTAB WRTAB CNTTAB&#x000D;&#x000A;ALARME EVENT</Keywords>
            <Keywords name="Words3">AI AO DIP8 DOP8 DIB BCDIN BCDOUT LOGI LOGO ANAI LOGI8 RDIOB WRIOB RDIOB INPUT TPRINT LOGO8</Keywords>
            <Keywords name="Words4">NAME ARCOD ARCODE ARCODB ENDS EXTRN CODE DATA FCODE</Keywords>
        </KeywordLists>
        <Styles>
            <WordsStyle name="DEFAULT" styleID="11" fgColor="000000" bgColor="FFFFFF" fontName="" fontStyle="0" />
