            <Prefix words1="no" words2="no" words3="yes" words4="yes" />
        </Settings>
        <KeywordLists>
            <Keywords name="Delimiters">&quot;00&quot;00</Keywords>
            <Keywords name="Folder+">{</Keywords>
            <Keywords name="Folder-">};</Keywords>
            <Keywords name="Operators">$</Keywords>
            <Keywords name="Comment">1/* 2*/ 0// 0#</Keywords>
            <Keywords name="Words1">
			    category
                channel
                cleaning-interval
                coresize
                datasize
                deallocate-on-exit
                dialup
                directory
                disable-algorithms
                dnssec-lookaside
                dnssec-must-be-secure
                dump-file
                edns-udp-size
                fake-iquery
                fetch-glue
                files
                has-old-clients
                heartbeat-interval
                host-statistics
                hostname
                interface-interval
                key-directory
                lame-ttl
                max-cache-size
                max-cache-ttl
                max-ixfr-log-size
                max-journal-size
                max-ncache-ttl
                max-refresh-time
                max-retry-time
                max-transfer-idle-in
                max-transfer-idle-out
                max-transfer-time-in
                max-transfer-time-out
