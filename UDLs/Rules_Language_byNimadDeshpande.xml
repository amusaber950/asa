<NotepadPlus>
    <UserLang name="Rules Language" ext="">
        <Settings>
            <Global caseIgnored="yes" />
            <TreatAsSymbol comment="yes" commentLine="yes" />
            <Prefix words1="no" words2="no" words3="no" words4="no" />
        </Settings>
        <KeywordLists>
            <Keywords name="Delimiters">&quot;&apos;0&quot;&apos;0</Keywords>
            <Keywords name="Folder+">FOR EACH IF ELSE</Keywords>
            <Keywords name="Folder-">END FOR; END IF;</Keywords>
            <Keywords name="Operators">- # % ( ) * . : ; @ [ ] ^ | + &lt; = &gt;</Keywords>
            <Keywords name="Comment"> 1/* 2*/ 0//</Keywords>
            <Keywords name="Words1">THEN IN NUMBER LIST FACTOR SET LEAVE NEXT SAVE OR AND SELECT DONE</Keywords>
            <Keywords name="Words2">INCLUDE CALL</Keywords>
            <Keywords name="Words3"> CLEAR</Keywords>
            <Keywords name="Words4">POST CANCEL LISTCOUNT LISTVALUE HASVALUE CEIL MIN DAY MONTHDIFF MONTHHOURS FLOOR ROUND ROUND COMPSUM MONTH DAYDIFF HASVALUE ROUND2VALUE ALL CHARGE INTO KWH TOTAL UNBILLED HISTCOUNT ROUNDDATE AS TABLE DETERMINANT IGNORE WEEK HISTMAX RSPRORATE HISTMIN SEASONVALUE HISTMINNZ SQROOT HISTVALUE STRING IDATTR SUMSEASON LF2KW USEREXIT LF2KWH WEEKDIFF LISTCOUNT YEAR MAX YEARSTR INTO WHEN</Keywords>
        </KeywordLists>
        <Styles>
            <WordsStyle name="DEFAULT" styleID="11" fgColor="000000" bgColor="FFFFFF" fontName="Courier New" fontStyle="0" />
            <WordsStyle name="FOLDEROPEN" styleID="12" fgColor="0000FF" bgColor="FFFFFF" fontStyle="0" />
            <WordsStyle name="FOLDERCLOSE" styleID="13" fgColor="0000FF" bgColor="FFFFFF" fontStyle="0" />
            <WordsStyle name="KEYWORD1" styleID="5" fgColor="0000FF" bgColor="FFFFFF" fontStyle="0" />
            <WordsStyle name="KEYWORD2" styleID="6" fgColor="FF0000" bgColor="FFFFFF" fontStyle="0" />
            <WordsStyle name="KEYWORD3" styleID="7" fgColor="008000" bgColor="FFFFFF" fontStyle="0" />
            <WordsStyle name="KEYWORD4" styleID="8" fgColor="FF8000" bgColor="FFFFFF" fontStyle="0" />
            <WordsStyle name="COMMENT" styleID="1" fgColor="808080" bgColor="FFFFFF" fontStyle="0" />
            <WordsStyle name="COMMENT LINE" styleID="2" fgColor="808080" bgColor="FFFFFF" fontName="Courier New" fontStyle="0" fontSize="10" />
            <WordsStyle name="NUMBER" styleID="4" fgColor="FF5604" bgColor="FFFFFF" fontName="Courier New" fontStyle="0" fontSize="10" />
            <WordsStyle name="OPERATOR" styleID="10" fgColor="FF0080" bgColor="FFFFFF" fontStyle="0" />
            <WordsStyle name="DELIMINER1" styleID="14" fgColor="008000" bgColor="FFFFFF" fontStyle="0" />
            <WordsStyle name="DELIMINER2" styleID="15" fgColor="FF0080" bgColor="FFFFFF" fontStyle="0" />
            <WordsStyle name="DELIMINER3" styleID="16" fgColor="000000" bgColor="FFFFFF" fontStyle="0" />
        </Styles>
    </UserLang>
</NotepadPlus>
