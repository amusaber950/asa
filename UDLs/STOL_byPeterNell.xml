<NotepadPlus>
	<UserLang name="STOL" ext="proc">
        <Settings>
            <Global caseIgnored="yes" />
            <TreatAsSymbol comment="no" commentLine="yes" />
            <Prefix words1="no" words2="no" words3="no" words4="yes" />
        </Settings>
        <KeywordLists>
            <Keywords name="Delimiters">&quot;&apos;0&quot;&apos;0</Keywords>
            <Keywords name="Folder+">then do</Keywords>
            <Keywords name="Folder-">endif enddo elseif</Keywords>
            <Keywords name="Operators">- ! &amp; ( ) * , . / ^ | + &lt; = &gt;</Keywords>
            <Keywords name="Comment">1 1 2 2 0;</Keywords>
            <Keywords name="Words1">aquire archive ask break blind bypass cfgmon clear close cmd continue convert date disable dumpfile enable endproc free global go goto gpib hotkey killproc let limits load local log mode open page pktdump pkttimeout playback plot proc purge quit raw rawtf read rem reset resync retrlim retry return send seqprt set setcoef setgrnd shoval sim snap speed start stop stripchart system tfdump timeon timeout unlock vc verify wait write until while if else</Keywords>
            <Keywords name="Words2">gmt int real time loge dec hex lower oct upper string substr env exec is_file nargs clock ue is_normal is_warn is_error is_stale linear state volts on off</Keywords>
            <Keywords name="Words3">abs acos asin atan bwand bwor bwinvert bwlshift bwrshift ceil concat cos delta evtmsg exists floor format getenv iscommand isdate isfloat isglobal isint islocal ismnemonic isnull isredhi isreadlo isstatic isstring issymbol istime isunsigned isvariable isyellowhi isyellowlo ln log lowercase max min mkdate mktime mod name sin sqrt strfdate strlen strtol substr tan todate tofloat toint tostring totime tounsigned uppercase</Keywords>
            <Keywords name="Words4">true false x</Keywords>
        </KeywordLists>
        <Styles>
            <WordsStyle name="DEFAULT" styleID="11" fgColor="000000" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="FOLDEROPEN" styleID="12" fgColor="0000FF" bgColor="FFFFFF" fontName="" fontStyle="1" />
            <WordsStyle name="FOLDERCLOSE" styleID="13" fgColor="0000FF" bgColor="FFFFFF" fontName="" fontStyle="1" />
            <WordsStyle name="KEYWORD1" styleID="5" fgColor="0000FF" bgColor="FFFFFF" fontName="" fontStyle="1" />
            <WordsStyle name="KEYWORD2" styleID="6" fgColor="8000FF" bgColor="FFFFFF" fontName="" fontStyle="1" />
            <WordsStyle name="KEYWORD3" styleID="7" fgColor="0080C0" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="KEYWORD4" styleID="8" fgColor="FF8040" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="COMMENT" styleID="1" fgColor="80FF80" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="COMMENT LINE" styleID="2" fgColor="008000" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="NUMBER" styleID="4" fgColor="FF8040" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="OPERATOR" styleID="10" fgColor="FF0000" bgColor="FFFFFF" fontName="" fontStyle="1" />
            <WordsStyle name="DELIMINER1" styleID="14" fgColor="AD5A5A" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="DELIMINER2" styleID="15" fgColor="FF8000" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="DELIMINER3" styleID="16" fgColor="000000" bgColor="FFFFFF" fontName="" fontStyle="0" />
        </Styles>
    </UserLang>
</NotepadPlus>